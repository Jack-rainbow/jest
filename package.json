{
  "name": "nuxt-element-dashboard",
  "version": "2.2.0",
  "description": "Nuxt2 + element UI project",
  "author": "levy <levy9527@qq.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "element-ui",
    "nuxt",
    "dashboard"
  ],
  "engines": {
    "npm": ">= 5.2.0"
  },
  "scripts": {
    "mock": "cross-env MODE=mock nuxt",
    "dev": "cross-env MODE=dev PORT=3333 nuxt",
    "mock:nologin": "cross-env MODE=mock PORT=3333 NO_LOGIN=1 nuxt",
    "dev:nologin": "cross-env MODE=dev NO_LOGIN=1 nuxt",
    "build": "cross-env MODE=prod nuxt build",
    "build:docker": "nuxt build",
    "build:page": "node ./build/new.js",
    "start": "cross-env HOST=0.0.0.0 PORT=4444 MODE=prod nuxt start",
    "start:docker": "nuxt start",
    "generate": "cross-env MODE=prod nuxt generate",
    "deploy": "now --npm --public",
    "clean": "rimraf test/coverage/**",
    "test": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test:cover": "babel-node ./node_modules/.bin/babel-istanbul cover _mocha -- tests/unit/components/* -R spec --recursive"
  },
  "dependencies": {
    "@femessage/el-form-renderer": "^1.3.0",
    "@femessage/theme-deepexi": "^2.4.11",
    "@femessage/upload-to-ali": "^1.4.0",
    "@nuxtjs/dotenv": "^1.3.0",
    "axios": "^0.18.0",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.24.1",
    "dayjs": "^1.8.12",
    "dotenv": "^6.0.0",
    "downloadjs": "^1.4.7",
    "el-data-table": "^1.2.3",
    "element-ui": "2.4.11",
    "js-cookie": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mocha": "^6.1.4",
    "nuxt": "^2.3.2"
  },
  "devDependencies": {
    "@nuxtjs/proxy": "^1.3.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-regenerator-runtime": "^6.5.0",
    "chai": "^4.2.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^8.1.0",
    "mochawesome": "^3.1.2",
    "prettier": "^1.16.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}
